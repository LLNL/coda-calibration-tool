<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@CommonGui.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <HBox maxHeight="35.0" prefHeight="35.0" prefWidth="800.0" spacing="5.0" VBox.vgrow="NEVER">
               <children>
                  <ComboBox fx:id="eventPairComboBox" prefWidth="200.0">
                     <HBox.margin>
                        <Insets left="5.0" top="5.0" />
                     </HBox.margin>
                  </ComboBox>
                  <Button fx:id="showMapButton" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#showMapWindow" text="Map">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Label fx:id="topTitleLabel" contentDisplay="TEXT_ONLY" textAlignment="CENTER" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
                     </HBox.margin></Label>
                  <AnchorPane HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                     <children>
                        <Button fx:id="downloadButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#downloadPlots" pickOnBounds="false" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
               </children>
            </HBox>
            <SplitPane dividerPositions="0.3" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" VBox.vgrow="ALWAYS">
               <items>
                  <StackPane fx:id="borderPane" pickOnBounds="false">
                     <children>
                        <StackPane fx:id="spectraRatioPlotNode" pickOnBounds="false" prefHeight="580.0" prefWidth="800.0" StackPane.alignment="CENTER" />
                        <Button fx:id="snapshotButton" mnemonicParsing="false" onAction="#screenshotPlots" pickOnBounds="false" StackPane.alignment="BOTTOM_RIGHT">
                           <StackPane.margin>
                              <Insets bottom="10.0" right="15.0" />
                           </StackPane.margin>
                        </Button>
                     </children>
                  </StackPane>
                  <SplitPane dividerPositions="0.5">
                     <items>
                        <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="200.0">
                           <items>
                              <StackPane fx:id="pairStressPlotNode" minHeight="100.0" minWidth="100.0" prefHeight="150.0" prefWidth="200.0" />
                              <StackPane fx:id="pairMomentPlotNode" minHeight="100.0" minWidth="100.0" prefHeight="150.0" prefWidth="200.0" />
                           </items>
                        </SplitPane>
                        <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="200.0">
                           <items>
                              <StackPane fx:id="jointStressPlotNode" minHeight="100.0" minWidth="100.0" prefHeight="150.0" prefWidth="200.0" />
                              <StackPane fx:id="jointMomentPlotNode" minHeight="100.0" minWidth="100.0" prefHeight="150.0" prefWidth="200.0" />
                           </items>
                        </SplitPane>
                     </items>
                  </SplitPane>
               </items>
            </SplitPane>
         </children>
      </VBox>
   </children>
</StackPane>
